@{

}

@model IEnumerable<FundModel>
<div class="text-center">

 <table>
  <tr>
   <th>日期</th>
   <th>上证指数</th>
   <th>平安银行(000001)</th>
   <th>贵州茅台(600519)</th>
   <th>中信建投(601066)</th>
   <th>华兴源创(688001)</th>
   <th>同达创业(600647)</th>
   <th>上证指数涨跌幅</th>
   <th>平安银行(000001)相对收益</th>
   <th>贵州茅台(600519)相对收益</th>
   <th>中信建投(601066)相对收益</th>
   <th>华兴源创(688001)相对收益</th>
   <th>同达创业(600647)相对收益</th>
  </tr>
		@{
			foreach (var item in @Model)
			{
			   <tr>
				<td width="100px">@item.CurrentDateTime.ToString("yyyy-MM-dd")</td>
				<td>@item.ShanghaiCompositeIndex.ToString()</td>
				<td>@item.PinganBank.ToString()</td>
				<td>@item.MoutaiChina.ToString()</td>
				<td>@item.CiciCitic.ToString()</td>
				<td>@item.SuzhouHYCTechnology.ToString()</td>
				<td>@item.ShanghaiTongdaVentureCapitalCo.ToString()</td>
				<td>@item.ShanghaiCompositeIndexCompute.ToString()<label>%</label></td>
				<td>@item.PinganBankRelativeGain.ToString()</td>
				<td>@item.MoutaiChinaRelativeGain.ToString()</td>
				<td>@item.CiciCiticRelativeGain.ToString()</td>
				<td>@item.SuzhouHYCTechnologyRelativeGain.ToString()</td>
				<td>@item.ShanghaiTongdaVentureCapitalCoRelativeGain.ToString()</td>
			   </tr>
			}
		}

 </table>
</div>

